{
	"name": "CopyAllCitiBikefiles",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CitiBikeData",
						"type": "DatasetReference"
					},
					"name": "CitiBikeAllData"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "CitiBikeUncompressed",
						"type": "DatasetReference"
					},
					"name": "CitiBikeAllBlobStorage"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionRootPath: 'https://s3.amazonaws.com/tripdata',",
				"     multiLineRow: true,",
				"     wildcardPaths:['https://s3.amazonaws.com/tripdata/*zip']) ~> CitiBikeAllData",
				"CitiBikeAllData sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:[''],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> CitiBikeAllBlobStorage"
			]
		}
	}
}